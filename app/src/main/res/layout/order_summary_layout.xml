<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/card_view"
    android:layout_width="match_parent"
    android:background="@color/button_text"
    android:layout_height="match_parent"
    >

    <RelativeLayout
        android:id="@+id/main"
        android:background="#ffffff"
        android:layout_width="match_parent"

        android:layout_height="wrap_content">
        <LinearLayout
            android:id="@+id/sellerMainLay"
            android:layout_width="fill_parent"
            android:background="@color/button_text"

            android:orientation="vertical"
            android:layout_height="wrap_content">
            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <Button
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="5dp"
                    android:id="@+id/sellerLabel"
                    style="?android:attr/borderlessButtonStyle"
                    android:paddingRight="20dp"
                    android:background="@drawable/custom_button"
                    android:layout_width="wrap_content"
                    android:layout_height="30dp"
                    android:text="@string/seller"
                    android:maxHeight="0dp"
                    android:textColor="#777777"
                    android:gravity="center"
                    android:textSize="12sp"

                    />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="Small Text"
                    android:padding="5dp"
                    android:gravity="center"
                    android:textColor="#777777"
                    android:layout_alignLeft="@+id/OverViewTitle"
                    android:id="@+id/sellerNameText"
                    android:layout_below="@+id/OverViewTitle"

                    />

            </LinearLayout>
            <!--<View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/edittext_color"/>-->

            <!--<LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                    <View
                        android:id="@+id/mainDivider"
                        android:layout_below="@+id/sellerLabel"
                        android:layout_width="match_parent"
                        android:background="#eeeeee"
                        android:layout_height="1dp"/>
            </LinearLayout>-->
        </LinearLayout>
        <View
            android:id="@+id/sellerDivider"
            android:layout_below="@+id/sellerMainLay"
            android:layout_width="match_parent"
            android:background="#dddddd"

            android:layout_height="1dp"/>

        <FrameLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageHolderLayout"
            android:layout_below="@+id/sellerDivider" >

            <ProgressBar
                style="?android:attr/progressBarStyleSmall"
                android:id="@+id/load_progress_bar"
                android:indeterminateDrawable="@drawable/progress_bar_load_more"
                android:layout_width="wrap_content"
                android:layout_gravity="center"
                android:layout_height="wrap_content"
                />

            <ImageView
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:id="@+id/mainImg"
                android:src="@drawable/test"
                android:scaleType="fitCenter"
                android:padding="4dp"
                android:layout_marginLeft="10dp"
                android:layout_marginBottom="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="10dp" />
        </FrameLayout>


        <!--<RatingBar
            android:layout_below="@+id/imageHolderLayout"
            android:id="@+id/main_prodRatingBar"
            style="?android:attr/ratingBarStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:numStars="5"
            android:rating="2.0"
            android:stepSize="1.0" />-->


        <TextView
            android:id="@+id/OverViewTitle"

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:maxLines="3"
            android:padding="8dp"
            android:text="title"
            android:textColor="#333333"

            android:textAppearance="?android:attr/textAppearanceSmall"
            android:layout_alignTop="@+id/imageHolderLayout"
            android:layout_toRightOf="@+id/imageHolderLayout"

            />

        <LinearLayout
            android:id="@+id/priceLay"
            android:layout_marginRight="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_alignParentRight="true"
            android:layout_alignBottom="@+id/sellerLabelText"

            >
            <TextView
                android:id="@+id/overviewText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="bottom"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text=""
                android:textColor="#999999"


                android:layout_alignBottom="@+id/sellerLabelText"
                android:layout_toLeftOf="@+id/quantity"
                />
            <TextView
                android:layout_weight="1"
                android:id="@+id/newPrice"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_alignParentRight="true"
                android:layout_marginRight="5dp"




                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text=""
                android:textColor="#ff3823"


                android:layout_alignTop="@+id/overviewText"
                />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/quantity_count"
            android:background="#eeeeee"
            android:layout_below="@+id/sellerLabelText"
            android:layout_alignRight="@+id/priceLay"
            android:layout_alignEnd="@+id/priceLay"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_margin="1dp"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:orientation="horizontal">
                <Button
                    android:id="@+id/quantity_dec"
                    android:background="#eeeeee"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:textColor="@color/dialog_button_cancel"
                    android:textStyle="bold"

                    android:text="-"/>
                <EditText
                    android:layout_width="50dp"

                    android:layout_height="fill_parent"
                    android:inputType="numberDecimal"
                    android:singleLine="true"
                    android:textColor="#333333"

                    android:cursorVisible="true"
                    android:maxLength="2"

                    android:imeActionLabel="Update"
                    android:gravity="center"

                    android:background="#FFFFFF"
                    style="@style/_EditText"
                    android:id="@+id/quantity"


                    android:layout_below="@+id/sellerLabelText"
                    android:layout_alignRight="@+id/priceLay"
                    android:layout_alignEnd="@+id/priceLay" />
                <Button
                    android:textColor="@color/dialog_button_cancel"
                    android:textStyle="bold"
                    android:id="@+id/quantity_inc"
                    android:background="#eeeeee"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:text="+"/>
                <!--
                        android:imeOptions="actionDone"
                -->
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_below="@+id/main_prodRatingBar"
            android:layout_marginTop="5dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:id="@+id/linearLayout"
            >

            <CheckBox
                android:visibility="gone"
                android:id="@+id/cross"
                android:button="@drawable/checkbox_btn_check_holo_light"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                />


        </LinearLayout>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="gone"
            android:layout_alignParentRight="true"
            android:layout_marginRight="5dp"
            android:padding="8dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="desc"
            android:textColor="#666"
            android:id="@+id/totalPrice"
            android:layout_below="@+id/quantity_count"
            />

        <TextView
            android:layout_width="wrap_content"
            android:paddingTop="10dp"
            android:paddingBottom="10dp"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="In Stock : 25 Items"
            android:gravity="center_vertical"


            android:id="@+id/stock"
            android:textSize="12dp"
            android:textColor="#999999"
            android:layout_marginTop="10dp"

            android:layout_below="@+id/quantity_count"
            android:layout_alignRight="@+id/quantity_count"
            android:layout_alignEnd="@+id/quantity_count" />

        <Button
            android:paddingLeft="4dp"
            android:id="@+id/sellerLabelText"
            android:layout_below="@+id/OverViewTitle"
            android:layout_alignLeft="@+id/OverViewTitle"
            android:paddingRight="20dp"
            android:background="@drawable/custom_background_green"
            android:layout_width="60dp"
            style="?android:attr/borderlessButtonStyle"
            android:layout_height="20dp"
            android:text="@string/discount"
            android:maxHeight="0dp"
            android:textColor="#ffffff"
            android:gravity="center"
            android:textSize="14dp"
            android:layout_margin="5dp"
            />

        <TextView
            android:layout_marginLeft="10dp"
            android:padding="5dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="12dp"
            android:text="[ REMOVE ]"
            android:textColor="#888888"
            android:id="@+id/remove"
            android:layout_alignTop="@+id/stock"
            android:layout_toRightOf="@+id/totalPrice"
            android:layout_toEndOf="@+id/totalPrice" />

        <!--<TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
