    <RelativeLayout
        android:id="@+id/footer"

        android:background="#fcfafa"
        android:layout_below="@+id/main"
        android:layout_width="match_parent"

        android:layout_height="wrap_content">



        <RelativeLayout
            android:id="@+id/additionLay"
            android:elevation="4dp"
            android:background="#fcfafa"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">





            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/edittext_color"/>

            <LinearLayout
                android:id="@+id/subtotalLay"

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:orientation="horizontal">






                <TextView
                    android:text="Sub Total"
                    android:textColor="#999999"
                    android:layout_weight="1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />



                <TextView
                    android:text="AED 2000"
                    android:layout_weight="1"
                    android:textColor="#666666"
                    android:gravity="right"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />


            </LinearLayout>



            <LinearLayout
                android:id="@+id/shipLay"
                android:layout_below="@+id/subtotalLay"

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingRight="10dp"
                android:paddingLeft="10dp"
                android:orientation="horizontal">





                <LinearLayout

                    android:layout_weight="1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <TextView
                        android:text="Shipping"
                        android:textColor="#999999"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />


                    <TextView
                        android:text="United Arab Emirates >"
                        android:padding="2dp"
                        android:textSize="12dp"
                        android:textColor="#999999"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />

                </LinearLayout>

                <TextView
                    android:text="AED 30"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:textColor="#666666"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />


            </LinearLayout>





            <LinearLayout

                android:layout_below="@+id/shipLay"
                android:id="@+id/totalLay"
                android:layout_marginTop="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingRight="10dp"
                android:paddingLeft="10dp"
                android:orientation="horizontal">






                <TextView
                    android:text="Total"
                    android:paddingBottom="10dp"
                    android:layout_weight="1"
                    android:textColor="#999999"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />



                <TextView
                    android:text="AED 2000"
                    android:textColor="#ff3823"
                    android:paddingBottom="10dp"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />


            </LinearLayout>
            <LinearLayout
                android:layout_marginBottom="10dp"
                android:layout_below="@+id/totalLay"
                android:id="@+id/fgvbxfgbf"
                android:layout_marginTop="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingRight="10dp"
                android:paddingLeft="10dp"
                android:orientation="horizontal">






                <TextView
                    android:text=""
                    android:paddingBottom="5dp"
                    android:layout_weight="1"
                    android:textColor="#999999"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />



                <TextView
                    android:text=""
                    android:textColor="#ff3823"
                    android:paddingBottom="5dp"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />


            </LinearLayout>
           <!-- <LinearLayout

                android:visibility="gone"
                android:layout_marginTop="5dp"
                android:layout_below="@+id/totalLay"
